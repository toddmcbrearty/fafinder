{
  "manifest_version": 2,

  "name": "Font Awesome Finder",
  "description": "Helps find those hard to find but beautiful font awesome icons",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "WTFIT?"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}